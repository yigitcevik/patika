1) SELECT DISTINCT replacement_cost FROM film;
2) SELECT COUNT(DISTINCT replacement_cost) FROM film;     Cevap -> 21
3) SELECT COUNT(*) FROM film
WHERE title LIKE 'T%' AND rating = 'G';     Cevap -> 9
4) SELECT COUNT(*) FROM country
WHERE country LIKE '_____';     Cevap -> 13
5) SELECT COUNT(*) FROM city
WHERE city ILIKE '%R';      Cevap -> 33
